    "@types/request-promise": "^4.1.43",
    "axios": "^1.3.4",
    "bluebird": "^3.7.1",
    "chance": "^1.1.11",
    "class-transformer": "^0.5.1",
    "debug": "^4.3.4",
    "image-size": "^1.0.2",
    "json-bigint": "^1.0.0",
    "lodash": "^4.17.20",
    "luxon": "^3.3.0",
    "react": "^18.2.0",
    "react-native": "^0.71.4",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.0",
    "snakecase-keys": "^5.4.5",
    "tough-cookie": "^4.1.2",
    "ts-custom-error": "^3.3.1",
